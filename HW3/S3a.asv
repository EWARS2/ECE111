clc, clear, format compact % Robot Tip Position (Forward Kinematics)
%q1 = 26; % Angles
%q2 = 38;
%q3 = 46;
[q1, q2, q3] = deal(26, 38, 46);
x0 = 0; % Origin
y0 = 0;
x1 = 2 * cosd(q1);
y1 = 2 * sind(q1);
x2 = x1 + cosd(q1 + q2);
y2 = y1 + sind(q1 + q2);
x3 = x2 + cosd(q1 + q2 + q3);
y3 = y2 + sind(q1 + q2 + q3);
disp('Tip Position')
disp([x3,y3])
plot([x0,x1,x2,x3],[y0,y1,y2,y3],'b.-')
%xlim([-3,3])